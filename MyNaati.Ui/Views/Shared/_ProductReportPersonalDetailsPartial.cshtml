@model MyNaati.Ui.ViewModels.Shared.BasePurchaseWizardModel

<div class="part">
    <table class="wide">
        <tr>
            <th style="width: 200px;">Customer Number</th>
            <td>@Model.NaatiNumber</td>
        </tr>
        <tr>
            <th>Order Reference Number</th>
            <td>@Model.ReferenceNumber</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>@Model.Name</td>
        </tr>
        <tr>
            <th>Delivery Details</th>
            <td>
            @{ 
                Html.RenderPartial("_DeliveryDetailsViewPartial", Model.DeliveryDetailsModel, new ViewDataDictionary{ { "SuppressHeader", true }});
            }
            </td>
        </tr>
    </table>
</div>
