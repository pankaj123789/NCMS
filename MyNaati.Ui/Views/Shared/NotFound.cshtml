@{
    ViewBag.Title = "Not found";
    Layout = "~/Views/Shared/MasterPages/_Layout.cshtml";

    ViewBag.Title = "Sorry, the page you tried cannot be found";
    ViewBag.BreadCrumb = new List<Tuple<string, string>>
{
new Tuple<string, string>("Not found", string.Empty)
};
    var pathAndQuery = HttpContext.Current.Request.Url.PathAndQuery;
    var webHome = pathAndQuery.Split('/')[1];
}

@section Head {
    @Styles.Render("~/Bundles/Styles")
}

@if (User.Identity == null|| !User.Identity.IsAuthenticated)
{
    <ol class="breadcrumb">
        <li>
            <a href="/@webHome">Home</a>
        </li>
        <li class="active">Not found</li>
    </ol>
    <hr>

    <div style="padding-left: 15px;">

        <h2>Sorry, the page you tried cannot be found</h2>
        <div class="text-center wrapper-xl">
            <span class="text-warning fa-5x">404</span>
            <div class="text-center wrapper">
                <h4>
                    Please try again or contact <a href="mailto://info@naati.com.au">info@naati.com.au</a><br>
                </h4>
            </div>
        </div>

    </div>
}
else
{
    <div style="padding-left: 15px;">
        <h2>@ViewBag.Title</h2>
        <div class="text-center wrapper-xl">
            <span class="text-warning fa-5x">404</span>
            <div class="text-center wrapper">
                <h4>
                    Please try again or contact <a href="mailto://info@naati.com.au">info@naati.com.au</a><br />
                </h4>
            </div>
        </div>
    </div>
}



