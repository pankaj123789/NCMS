@model MyNaati.Ui.ViewModels.PDListing.WizardModel
<div class="row">
    <div class="col-sm-6">
        <h3 class="text-primary">Contact Details</h3>
        <div class="table-responsive">
            <table class="table table-bordered">
                <thead class="bg-primary">
                    <tr>
                        <th>Type</th>
                        <th>Contact</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var con in Model.Phones.Where(c => c.InDirectory))
                    {
                        <tr>
                           
                            <td>@con.Contact</td>
                        </tr>
                    }
                    @foreach (var con in Model.Emails.Where(c => c.InDirectory))
                    {
                        <tr>
                        
                            <td>@con.Contact</td>
                        </tr>
                    }
                    @if (Model.Website != null)
                    {
                        <tr>
                            <td>Website</td>
                            <td>@Model.Website.Contact</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
